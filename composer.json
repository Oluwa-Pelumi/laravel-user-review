{
    "name": "dgvai/laravel-user-review",
    "description": "A Laravel Package for User Review on Models, like Product review for ecommerce systems.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Jalal Uddin",
            "email": "dgvai-git@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "laravel/framework": "^8.0",
            "dgvai/laravel-user-review": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "DGvai\\Review\\": "src"
        }
    },
  "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Oluwa-Pelumi/laravel-user-review"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DGvai\\Review\\ReviewerServiceProvider"
            ]
        }
    }
}
